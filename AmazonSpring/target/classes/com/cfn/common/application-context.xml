	<bean class="com.cfn.beans.courier.BluedartCorier" id="bluedartCorier"/>
	<bean class="com.cfn.beans.courier.DTDCCourier" id="dTDCCourier"/>
	<bean class="com.cfn.model.AmazonOrder" id="amazonOrder"/>

</beans>
